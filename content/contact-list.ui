<?xml version="1.0" encoding="UTF-8"?>

<interface>
    <template class="ContactList" parent="GtkBox">
        <child>
            <object class="GtkListBox" id="tasks_list">
                <property name="selection-mode">none</property>
                <property name="vexpand">True</property>
                <style>
                    <class name="contacts-contact-list" />
                </style>

                <child>
                    <object class="GtkBox">
                        <property name="spacing">12</property>
                        <property name="orientation">horizontal</property>
                        <property name="margin-start">6</property>
                        <property name="margin-top">6</property>
                        <property name="margin-bottom">6</property>

                        <child>
                            <object class="AdwAvatar">
                                <property name="text">Jakob</property>
                                <property name="size">48</property>
                                <property name="show-initials">True</property>
                            </object>
                        </child>

                        <child>
                            <object class="GtkLabel">
                                <property name="label">Jakob</property>
                            </object>
                        </child>

                        <child>
                            <object class="GtkButton">
                                <property name="icon-name">call-start</property>
                                <property name="valign">center</property>
                                <property name="halign">end</property>
                                <property name="hexpand">True</property>
                                <property name="tooltip-text" translatable="yes">Play the Previous
                                    Song</property>
                                <style>
                                    <class name="circular" />
                                </style>
                            </object>
                        </child>

                        <child>
                            <object class="GtkButton">
                                <property name="icon-name">camera-video</property>
                                <property name="valign">center</property>
                                <property name="halign">end</property>
                                <property name="margin-end">12</property>
                                <property name="tooltip-text" translatable="yes">Play the Previous
                                    Song</property>
                                <style>
                                    <class name="circular" />
                                </style>
                            </object>
                        </child>

                    </object>
                </child>

                <child>
                    <object class="GtkBox">
                        <property name="spacing">12</property>
                        <property name="orientation">horizontal</property>
                        <property name="margin-start">6</property>
                        <property name="margin-top">6</property>
                        <property name="margin-bottom">6</property>

                        <child>
                            <object class="AdwAvatar">
                                <property name="text">Felicitas</property>
                                <property name="size">48</property>
                                <property name="show-initials">True</property>
                            </object>
                        </child>

                        <child>
                            <object class="GtkLabel">
                                <property name="label">Felicitas</property>
                            </object>
                        </child>

                        <child>
                            <object class="GtkButton">
                                <property name="icon-name">call-start</property>
                                <property name="valign">center</property>
                                <property name="halign">end</property>
                                <property name="hexpand">True</property>
                                <property name="tooltip-text" translatable="yes">Play the Previous
                                    Song</property>
                                <style>
                                    <class name="circular" />
                                </style>
                            </object>
                        </child>

                        <child>
                            <object class="GtkButton">
                                <property name="icon-name">camera-video</property>
                                <property name="valign">center</property>
                                <property name="halign">end</property>
                                <property name="margin-end">12</property>
                                <property name="tooltip-text" translatable="yes">Play the Previous
                                    Song</property>
                                <style>
                                    <class name="circular" />
                                </style>
                            </object>
                        </child>

                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>